pkgname=tea-utils
pkgver=0.2.0
pkgrel=1
pkgdec="tea-utils"
arch=("x86_64")
license=('BSD' 'MIT' 'GPL')
source=("https://github.com/SkywalkerSW5/teaOS/raw/master/teaOS-builds/$pkgname-$pkgver.tar.xz")


build()
	 {

	mv $pkgname-$pkgver.tar.xz $srcdir/
	cd $srcdir
	tar xvf $pkgname-$pkgver.tar.xz
	rm $pkgname-$pkgver.tar.xz

         }
package() 
	 {
	
	 cp -r $srcdir/* $pkgdir
	 cd $pkgdir
         }
