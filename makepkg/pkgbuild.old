pkgname=tea-utils
pkgver=0.2.0
pkgrel=1
pkgdec="teaOS Pkg for the ISO"
arch=("x86_64")
license=('BSD')
#source=("./$pkgname-$pkgver.tar.xz")
source=("https://github.com/SkywalkerSW5/teaOS/raw/master/teaOS-builds/$pkgname-$pkgver.tar.xz")
#md5sums=('accc85b44d425a776d0b0ca63e7cd2c9')
prepare() {
	
	mkdir $pkgname-$pkgver
	tar xvf ./$pkgname-$pkgver.tar.xz -C ./$pkgname-$pkgver
	
}
build() {
	
	cd $pkgname-$pkgver/
	

}
package() {
	cd $pkgdir
	mkdir -p "teaOS"
	cp $srcdir/$pkgname-$pkgver.tar.xz $pkgdir/
	tar xvf $pkgname-$pkgver.tar.xz

	
	
}
post_install() {
	clear 
	echo "ewww"
}
